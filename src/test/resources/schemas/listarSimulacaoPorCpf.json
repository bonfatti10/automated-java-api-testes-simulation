{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "id",
    "nome",
    "cpf",
    "email",
    "valor",
    "parcelas",
    "seguro"
  ],
  "properties": {
    "id": {
      "type": "integer",
      "default": 0,
      "title": "The id Schema",
      "examples": [
        11
      ]
    },
    "nome": {
      "type": "string",
      "default": "",
      "title": "The nome Schema",
      "examples": [
        "Fulano"
      ]
    },
    "cpf": {
      "type": "string",
      "default": "",
      "title": "The cpf Schema",
      "examples": [
        "66414919004"
      ]
    },
    "email": {
      "type": "string",
      "default": "",
      "title": "The email Schema",
      "examples": [
        "fulano@gmail.com"
      ]
    },
    "valor": {
      "type": "number",
      "default": 0.0,
      "title": "The valor Schema",
      "examples": [
        11000.00
      ]
    },
    "parcelas": {
      "type": "integer",
      "default": 0,
      "title": "The parcelas Schema",
      "examples": [
        3
      ]
    },
    "seguro": {
      "type": "boolean",
      "default": false,
      "title": "The seguro Schema",
      "examples": [
        true
      ]
    }
  },
  "examples": [{
    "id": 11,
    "nome": "Fulano",
    "cpf": "66414919004",
    "email": "fulano@gmail.com",
    "valor": 11000.00,
    "parcelas": 3,
    "seguro": true
  }]
}